CREATE TABLE Account (accountId BIGINT NOT NULL, accountName VARCHAR(100) NOT NULL, deleted SMALLINT, lastModifiedDate TIMESTAMP, status SMALLINT, COUNTRY_ID BIGINT, GEOGRAPHY_ID BIGINT, LASTMODIFIEDBY_ID BIGINT, unit_id BIGINT NOT NULL, PRIMARY KEY (accountId), CONSTRAINT U_ACCOUNT_ACCOUNTN UNIQUE (accountName, unit_id));
CREATE TABLE Account_locations (ACCOUNT_ACCOUNTID BIGINT, element VARCHAR(50));
CREATE TABLE Account_User (COORDINATORACCOUNTS_ACCOUNTID BIGINT, ACCOUNTCOORDINATORS_ID BIGINT, DPEACCOUNTS_ACCOUNTID BIGINT, ACCOUNTDPES_ID BIGINT, ISSUEA_ACCOUNTID BIGINT, ACCOUNTISSUEADMINISTRATORS_ID BIGINT);
CREATE TABLE ActivationType (id BIGINT NOT NULL, name VARCHAR(100) NOT NULL, PRIMARY KEY (id), CONSTRAINT U_CTVTTYP_NAME UNIQUE (name));
CREATE TABLE Configuration (name VARCHAR(100) NOT NULL, value VARCHAR(200), PRIMARY KEY (name));
CREATE TABLE Country (id BIGINT NOT NULL, deleted SMALLINT, name VARCHAR(100) NOT NULL, geography_id BIGINT NOT NULL, PRIMARY KEY (id), CONSTRAINT U_COUNTRY_NAME UNIQUE (name), CONSTRAINT U_COUNTRY_NAME1 UNIQUE (name, geography_id));
CREATE TABLE Country_User (LEADCOUNTRIES_ID BIGINT, COUNTRYLEADS_ID BIGINT);
CREATE TABLE Evidence (id BIGINT NOT NULL, content BLOB, evidenceType SMALLINT, lastModifiedDate TIMESTAMP NOT NULL, name VARCHAR(100), LASTMODIFIEDBY_ID BIGINT NOT NULL, PRIMARY KEY (id));
CREATE TABLE Family (id BIGINT NOT NULL, name VARCHAR(100) NOT NULL, PRIMARY KEY (id), CONSTRAINT U_FAMILY_NAME UNIQUE (name));
CREATE TABLE Geography (id BIGINT NOT NULL, deleted SMALLINT, name VARCHAR(100) NOT NULL, PRIMARY KEY (id), CONSTRAINT U_GOGRPHY_NAME UNIQUE (name));
CREATE TABLE Geography_User (LEADGEOGRAPHIES_ID BIGINT, GEOGRAPHYLEADS_ID BIGINT);
CREATE TABLE IssueDocument (issueDocumentId BIGINT NOT NULL, lastModifiedDate TIMESTAMP, name VARCHAR(100) NOT NULL, status SMALLINT, LASTMODIFIEDBY_ID BIGINT, PROCESS_ID BIGINT, unit_id BIGINT NOT NULL, PRIMARY KEY (issueDocumentId), CONSTRAINT U_SSDCMNT_NAME UNIQUE (name, unit_id));
CREATE TABLE IssueRequest (issueRequestId BIGINT NOT NULL, comment VARCHAR(512), interestedParty VARCHAR(100), lastModifiedDate TIMESTAMP, status SMALLINT, ACCOUNT_ACCOUNTID BIGINT, DPE_ID BIGINT, ISSUEADMINISTRATOR_ID BIGINT, ISSUEOWNER_ID BIGINT, LASTMODIFIEDBY_ID BIGINT, PROCESS_ID BIGINT, PRIMARY KEY (issueRequestId));
CREATE TABLE OPENJPA_SEQUENCE_TABLE (ID SMALLINT NOT NULL, SEQUENCE_VALUE BIGINT, PRIMARY KEY (ID));
CREATE TABLE Platform (id BIGINT NOT NULL, deleted SMALLINT, name VARCHAR(100) NOT NULL, geography_id BIGINT NOT NULL, PRIMARY KEY (id), CONSTRAINT U_PLTFORM_NAME UNIQUE (name, geography_id));
CREATE TABLE Process (id BIGINT NOT NULL, initialActions VARCHAR(512), name VARCHAR(100) NOT NULL, PRIMARY KEY (id), CONSTRAINT U_PROCESS_NAME UNIQUE (name));
CREATE TABLE Question (questionId BIGINT NOT NULL, answer SMALLINT, comments VARCHAR(512), expirationDate TIMESTAMP, lastModifiedDate TIMESTAMP, reassignCount BIGINT, rejectCount BIGINT, status SMALLINT, ASSIGNEE_ID BIGINT, ISSUEDOCUMENT_ISSUEDOCUMENTID BIGINT, ISSUEREQUEST_ISSUEREQUESTID BIGINT, LASTMODIFIEDBY_ID BIGINT, QUESTIONCUSTOMIZATION_ID BIGINT, QUESTIONPROFILE_ID BIGINT, REQUEST_REQUESTID BIGINT, REQUESTPROFILE_ID BIGINT, SUBCATEGORY_ID BIGINT, PRIMARY KEY (questionId));
CREATE TABLE QuestionCustomization (id BIGINT NOT NULL, lastModifiedDate TIMESTAMP, localEvidenceRequirement CLOB(1M), localGuidelines CLOB(1M), questionNumber BIGINT NOT NULL, team VARCHAR(50), version VARCHAR(20) NOT NULL, activationType_id BIGINT NOT NULL, LASTMODIFIEDBY_ID BIGINT, unit_id BIGINT NOT NULL, PRIMARY KEY (id), CONSTRAINT U_QSTNZTN_UNIT_ID UNIQUE (unit_id, questionNumber, activationType_id, version));
CREATE TABLE QuestionProfile (id BIGINT NOT NULL, evidenceRequirement CLOB(1M), guidelines CLOB(1M), isEvidenceRequired SMALLINT, lastModifiedDate TIMESTAMP, processBullet VARCHAR(100), question VARCHAR(1024), questionNumber BIGINT NOT NULL, severity SMALLINT, activationType_id BIGINT NOT NULL, applicableFamilyScenarios BLOB, LASTMODIFIEDBY_ID BIGINT, PROCESS_ID BIGINT, questionSet_id BIGINT NOT NULL, PRIMARY KEY (id), CONSTRAINT U_QSTNRFL_QUESTION UNIQUE (questionNumber, activationType_id, questionSet_id));
CREATE TABLE QuestionSet (id BIGINT NOT NULL, deleted SMALLINT, deployBy TIMESTAMP, lastModifiedDate TIMESTAMP, published TIMESTAMP, status SMALLINT, uploaded TIMESTAMP, version VARCHAR(20) NOT NULL, LASTMODIFIEDBY_ID BIGINT, PRIMARY KEY (id), CONSTRAINT U_QUSTNST_VERSION UNIQUE (version));
CREATE TABLE Question_Evidence (QUESTIONS_QUESTIONID BIGINT, EVIDENCES_ID BIGINT);
CREATE TABLE Request (requestId BIGINT NOT NULL, changeReference VARCHAR(200), configId VARCHAR(50), coordinatorComments VARCHAR(512), dpeComments VARCHAR(512), draft SMALLINT, expirationDate TIMESTAMP, hostName VARCHAR(100), installDate TIMESTAMP, ipAddress VARCHAR(100), lastModifiedDate TIMESTAMP, location VARCHAR(50), name VARCHAR(100), project VARCHAR(50), proofOfInstallDate VARCHAR(512), requestorComments VARCHAR(512), serialNumber VARCHAR(50), serverDescription VARCHAR(200), status SMALLINT, ACCOUNT_ACCOUNTID BIGINT, COORDINATOR_ID BIGINT, DPEAPPROVER_ID BIGINT, LASTMODIFIEDBY_ID BIGINT, PLATFORM_ID BIGINT, PROFILE_ID BIGINT, PROJECTMANAGER_ID BIGINT, REQUESTOR_ID BIGINT, SCENARIO_ID BIGINT, PRIMARY KEY (requestId));
CREATE TABLE RequestProfile (id BIGINT NOT NULL, description VARCHAR(512), lastModifiedDate TIMESTAMP, name VARCHAR(100), account_id BIGINT NOT NULL, COORDINATOR_ID BIGINT, DPEAPPROVER_ID BIGINT, LASTMODIFIEDBY_ID BIGINT, PLATFORM_ID BIGINT, scenario_id BIGINT NOT NULL, subCategory_id BIGINT NOT NULL, PRIMARY KEY (id), CONSTRAINT U_RQSTRFL_ACCOUNT_ UNIQUE (account_id, scenario_id, subCategory_id));
CREATE TABLE Request_SubCategory (REQUEST_REQUESTID BIGINT, SUBCATEGORIES_ID BIGINT);
CREATE TABLE Scenario (id BIGINT NOT NULL, name VARCHAR(100) NOT NULL, ACTIVATIONTYPE_ID BIGINT, PRIMARY KEY (id), CONSTRAINT U_SCENRIO_NAME UNIQUE (name));
CREATE TABLE SubCategory (id BIGINT NOT NULL, deleted SMALLINT, name VARCHAR(100) NOT NULL, family_id BIGINT NOT NULL, unit_id BIGINT NOT NULL, PRIMARY KEY (id), CONSTRAINT U_SBCTGRY_NAME UNIQUE (name, family_id, unit_id));
CREATE TABLE Unit (id BIGINT NOT NULL, deleted SMALLINT, name VARCHAR(100) NOT NULL, QUESTIONSET_ID BIGINT, country_id BIGINT NOT NULL, PRIMARY KEY (id), CONSTRAINT U_UNIT_NAME UNIQUE (name), CONSTRAINT U_UNIT_NAME1 UNIQUE (name, country_id));
CREATE TABLE Unit_issueOwners (UNIT_ID BIGINT, VALUE_ID BIGINT, KEY0_ID BIGINT);
CREATE TABLE Unit_User (LEADUNITS_ID BIGINT, UNITLEADS_ID BIGINT);
CREATE TABLE User (id BIGINT NOT NULL, email VARCHAR(100) NOT NULL, isGlobalLead SMALLINT, isGroup SMALLINT NOT NULL, isSuperUser SMALLINT, name VARCHAR(100) NOT NULL, PRIMARY KEY (id), CONSTRAINT U_USER_EMAIL UNIQUE (email), CONSTRAINT U_USER_NAME UNIQUE (name, isGroup));
