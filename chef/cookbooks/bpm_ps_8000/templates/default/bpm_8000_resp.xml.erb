<server>
<repository location=<%= @repository_location %>/>
</server>
<profile id='IBM WebSphere Application Server Network Deployment V8.0' installLocation=<%= @installLocation %>>
<data key='eclipseLocation' value=<%= @eclipseLocation %>/>
<data key='user.import.profile' value='false'/>
<data key='cic.selector.os' value=<%= @os %>/>
<data key='cic.selector.arch' value=<%= @arch %>/>
<data key='cic.selector.ws' value=<%= @ws %>/>
<data key='cic.selector.nl' value='en'/>
</profile>
<install modify='false'>
